        <TextBox x:Name="TextBoxValeurParam2" HorizontalAlignment="Left" Height="36" Margin="237,153,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" TextChanged="TextBox_TextChanged"/>
        <Label x:Name="LabelParam3" Content="Param3" HorizontalAlignment="Left" Margin="67,189,0,0" VerticalAlignment="Top" Width="114" Height="36"/>
        <TextBox x:Name="TextBoxValeurParam3" HorizontalAlignment="Left" Height="31" Margin="237,194,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" TextChanged="TextBox_TextChanged"/>
        <Label x:Name="LabelResult" Content="" HorizontalAlignment="Left" Margin="67,283,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.629,-0.71" Width="405"/>
        <Label x:Name="LabelTestParam" Content="" HorizontalAlignment="Left" Margin="237,230,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
